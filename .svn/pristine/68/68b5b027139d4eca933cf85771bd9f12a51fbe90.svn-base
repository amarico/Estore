package goodsDe;
//ÊÖ»ú±í
public class Phone {
	
	private int gid;
    private String brand;
    private String internet;
    private String host;
    private int screen;
    private String system;
    
    public Phone(){
    	
    }
    
	public Phone(int gid, String brand, String internet, String host,
			int screen, String system) {
		//super();
		this.gid = gid;
		this.brand = brand;
		this.internet = internet;
		this.host = host;
		this.screen = screen;
		this.system = system;
	}

	public int getGid() {
		return gid;
	}
	public void setGid(int gid) {
		this.gid = gid;
	}
	public String getBrand() {
		return brand;
	}
	public void setBrand(String brand) {
		this.brand = brand;
	}
	public String getInternet() {
		return internet;
	}
	public void setInternet(String internet) {
		this.internet = internet;
	}
	public String getHost() {
		return host;
	}
	public void setHost(String host) {
		this.host = host;
	}
	public int getScreen() {
		return screen;
	}
	public void setScreen(int screen) {
		this.screen = screen;
	}
	public String getSystem() {
		return system;
	}
	public void setSystem(String system) {
		this.system = system;
	}
    
    
    

}
