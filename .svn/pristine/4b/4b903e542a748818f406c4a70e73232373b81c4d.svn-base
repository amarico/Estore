package entity;

public class Recommendation {

	
	//商品推荐表
	private int rid;
	private String rname;
	private int fid;
	
	
	
	public Recommendation(){
		
	}
	
	
	public Recommendation(int rid, String rname, int fid) {
		//super();
		this.rid = rid;
		this.rname = rname;
		this.fid = fid;
	}
	public int getRid() {
		return rid;
	}
	public void setRid(int rid) {
		this.rid = rid;
	}
	public String getRname() {
		return rname;
	}
	public void setRname(String rname) {
		this.rname = rname;
	}
	public int getFid() {
		return fid;
	}
	public void setFid(int fid) {
		this.fid = fid;
	}
	
	
	
	
}
